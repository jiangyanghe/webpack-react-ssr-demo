    "cross-env": "^5.2.0",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.19.1",
    "memory-fs": "^0.4.1",
    "react": "^16.8.4",
    "react-hot-loader": "^4.8.0",
    "rimraf": "^2.6.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
